<ivy-module>

    <info publication="20110411103700">
        <license name="Apache License, Version 2.0" url="http://www.apache.org/licenses/LICENSE-2.0"/>
        <description homepage="http://jettison.codehaus.org/">
            <p>
                Jettison is a collection of Java APIs (like STaX and DOM) which read and write JSON.
                This allows nearly transparent enablement of JSON based web services in services
                frameworks like CXF or XML serialization frameworks like XStream.
            </p>
        </description>
    </info>

    <publications>
        <artifact/>
        <artifact type="source" ext="zip"/>
        <artifact type="javadoc" ext="zip"/>
    </publications>

    <dependencies>
        <dependency org="javax.xml.stream" name="sjsxp" rev="1.+" conf="default->default"/> 
    </dependencies>

</ivy-module>
