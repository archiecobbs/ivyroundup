<ivy-module>

    <info publication="20111005060356">
        <license name="Apache License, Version 2.0" url="http://www.apache.org/licenses/LICENSE-2.0"/>
                <description homepage="http://cxf.apache.org">
                Apache CXF Bundle Jar
                </description>
    </info>

        <configurations>
                <conf name="default" visibility="public" description="runtime dependencies and master artifact can be used with this conf"/>
                <conf name="http-binding" visibility="public" description="For HTTP Binding support" extends="default"/>
                <conf name="jax-rs" visibility="public" description="not implemented"/>
                <conf name="aegis" visibility="public" description="aegis data binding not implemented"/>
                <conf name="jms" visibility="public" description="For JMS transport" extends="default"/>
                <conf name="corba" visibility="public" description="For CORBA support" extends="default"/>
                <conf name="javascript" visibility="public" description="For JavaScript functionality" extends="default"/>
                <conf name="security" visibility="public" description="For WS-Security support, incl SAML"/>
        </configurations>
        <publications>
                <artifact name="cxf-bundle" type="jar" ext="jar" conf="default"/>
        </publications>
        <dependencies>
                <dependency org="org.apache.ws.commons" name="XmlSchema" rev="2.0.1" force="true" conf="default->default">
                        <exclude org="org.apache.bcel" module="bcel" name="*" type="*" ext="*" conf="" matcher="exact"/>
                        <exclude org="xalan" module="xalan" name="*" type="*" ext="*" conf="" matcher="exact"/>
                </dependency>
                <dependency org="org.codehaus.woodstox" name="woodstox-core-asl" rev="4.1.1" force="true" conf="default->default">
                        <exclude org="javax.xml.stream" module="stax-api" name="*" type="*" ext="*" conf="" matcher="exact"/>
                        <exclude org="stax" module="stax-api" name="*" type="*" ext="*" conf="" matcher="exact"/>
                </dependency>
                <dependency org="org.codehaus.woodstox" name="stax2-api" rev="3.1.1" force="true" conf="default->default">
                        <exclude org="javax.xml.stream" module="stax-api" name="*" type="*" ext="*" conf="" matcher="exact"/>
                </dependency>
                <dependency org="javax.xml.bind" name="jaxb" rev="2.2" force="true" conf="default->default">
                        <exclude org="javax.activation" module="jaf" name="*" type="*" ext="*" conf="" matcher="exact"/>
                        <exclude org="javax.xml.stream" module="stax-api" name="*" type="*" ext="*" conf="" matcher="exact"/>
                        <exclude org="javax.xml" module="jsr173" name="*" type="*" ext="*" conf="" matcher="exact"/>
                        <exclude org="javax.xml.bind" module="jsr173_api" name="*" type="*" ext="*" conf="" matcher="exact"/>
                </dependency>
                <dependency org="net.sourceforge.wsdl4j" name="wsdl4j" rev="1.6.2" force="true" conf="default->default"/>
                <dependency org="org.apache.commons" name="commons-collections" rev="3.2.2" force="true" conf="default->default"/>
                <dependency org="org.apache.commons" name="commons-lang" rev="2.6" force="true" conf="default->default"/>
                <dependency org="org.apache.neethi" name="neethi" rev="3.0.1" force="true" conf="default->default">
                        <exclude org="org.apache.ws.security" module="wss4j" name="*" type="*" ext="*" conf="" matcher="exact"/>
                        <exclude org="org.codehaus.woodstox" module="woodstox-core-asl" name="*" type="*" ext="*" conf="" matcher="exact"/>
                </dependency>
                <dependency org="org.apache.xml" name="xml-resolver" rev="1.2" force="true" conf="default->default"/><!--should be 2.4.3?-->
                <dependency org="org.objectweb.asm" name="asm" rev="3.1" force="true" conf="default->core,util"/><!--should be 3.3?-->

                <dependency org="org.springframework" name="spring" rev="3.0.6" force="true" conf="default->web,beans,aop,asm,context,core,expression,web"/>
                <dependency org="org.springframework" name="spring" rev="3.0.6" force="true" conf="jms->jms,tx"/>

                <dependency org="org.aopalliance" name="aopalliance" rev="1.0" force="true" conf="default->default"/>
                <dependency org="javax.xml.soap" name="saaj" rev="1.3" force="true" conf="default->default"><!--should be 1.3.4?-->
                        <exclude org="javax.activation" module="*" name="*" type="*" ext="*" conf="" matcher="exact"/>
                </dependency>
                <dependency org="org.jvnet.mimepull" name="mimepull" rev="1.4" force="true" conf="default->default"/>
                <dependency org="org.antlr" name="antlr" rev="2.7.7" force="true" conf="corba->default"/>
                <dependency org="org.apache.xmlbeans" name="xmlbeans" rev="2.4.0" force="true" conf="default->default">
                        <exclude org="javax.xml.stream" module="jsr173_api" name="*" type="*" ext="*" conf="" matcher="exact"/>
                </dependency>
                <dependency org="javax.ws.rs" name="jsr311-api" rev="1.1.1" force="true" conf="default->default"/>
                <dependency org="org.codehaus.jettison" name="jettison" rev="1.3" force="true" conf="http-binding->default">
                        <exclude org="javax.xml.stream" module="jsr173_api" name="*" type="*" ext="*" conf="" matcher="exact"/>
                </dependency>
                <dependency org="org.slf4j" name="slf4j" rev="1.6.1" force="true" conf="default->default"/>
                <dependency org="org.codehaus.jra" name="jra" rev="1.0-alpha-4" force="true" conf="http-binding->default"/>
                <dependency org="org.mozilla" name="rhino" rev="1.7R2" force="true" conf="javascript->default"/>
                <dependency org="org.apache.ws.security" name="wss4j" rev="1.6.3" force="true" conf="security->default">
                        <exclude org="org.velocity" module="velocity" name="*" type="*" ext="*" conf="" matcher="exact"/>
                        <exclude org="org.owasp.esapi" module="esapi" name="*" type="*" ext="*" conf="" matcher="exact"/>
                        <exclude org="org.apache.commons" module="commons-codec" name="*" type="*" ext="*" conf="" matcher="exact"/>
                        <exclude org="org.apache.commons" module="commons-httpclient" name="*" type="*" ext="*" conf="" matcher="exact"/>
                        <exclude org="org.apache.xerces" module="xerces" name="*" type="*" ext="*" conf="" matcher="exact"/>
                        <exclude org="org.apache.xml" module="xalan-j" name="*" type="*" ext="*" conf="" matcher="exact"/>
                        <exclude org="org.slf4j" module="*" name="*" type="*" ext="*" conf="" matcher="exact"/>
                        <exclude org="org.bouncycastle" module="bcprov-jdk15" name="*" type="*" ext="*" conf="" matcher="exact"/>
                        <exclude org="ca.juliusdavies" module="not-yet-commons-ssl" name="*" type="*" ext="*" conf="" matcher="exact"/>
                        <exclude org="net.jcip" module="jcip-annotations" name="*" type="*" ext="*" conf="" matcher="exact"/>
                </dependency>
                <dependency org="org.apache.xml" name="xalan-j" rev="2.7.1" force="true" conf="default->default">
                        <exclude org="org.apache.xml" module="xml-commons-external" name="*" type="*" ext="*" conf="" matcher="exact"/>
                </dependency>
                <dependency org="com.sun.xml.fastinfoset" name="FastInfoset" rev="1.2.9" force="true" conf="default->default">
                        <exclude org="javax.xml.stream" module="jsr173_api" name="*" type="*" ext="*" conf="" matcher="exact"/>
                </dependency>
        </dependencies>
</ivy-module>
