<ivy-module>

    <info publication="20171214000000">
        <license name="Apache License, Version 2.0" url="http://www.apache.org/licenses/LICENSE-2.0"/>
        <description homepage="https://asterisk-java.org/">
        <p>
            The Asterisk-Java package consists of a set of Java classes that allow
            you to easily build Java applications that interact with an Asterisk PBX Server.
        </p>
        <p>
            Asterisk-Java supports most features of the following Asterisk interfaces:
            <ul>
                <li>Fast Asterisk Gateway Interface (FastAGI)</li>
                <li>Asterisk Manager interface (AMI)</li>
                <li>Inter-Asterisk eXchange (IAX) Protocol</li>
            </ul>
        </p>
        </description>
    </info>

    <publications>
        <artifact/>
        <artifact type="source" ext="zip"/>
        <artifact type="javadoc" ext="zip"/>
    </publications>

    <dependencies>
        <dependency org="org.slf4j" name="slf4j" rev="1.7+" conf="default->default"/>
    </dependencies>

</ivy-module>
