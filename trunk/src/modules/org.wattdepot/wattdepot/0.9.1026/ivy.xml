<ivy-module rev="$Id$">

    <info publication="20091026120000">
        <license name="BSD License" url="http://opensource.org/licenses/bsd-license.php"/>
        <description homepage="http://code.google.com/p/wattdepot/">
WattDepot is a RESTful web service that collects electricity data (such as current power utilization or cumulative power utilization) from meters and stores it in a database. The data can then be retrieved by other tools for visualization and analysis.
        </description>
    </info>

    <publications>
        <artifact name="wattdepot-server" />
        <artifact name="wattdepot-converter-oscar" />
        <artifact name="wattdepot-sensor-tabular" />
        <artifact name="wattdepot-sensor-bmo" />
        <artifact name="wattdepot-lib-client" />
        <artifact name="source" type="source" ext="zip"/>
        <artifact name="javadoc" type="javadoc" ext="zip"/>
    </publications>

</ivy-module>
