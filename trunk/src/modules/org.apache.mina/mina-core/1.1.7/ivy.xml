<ivy-module rev="$Id$">

    <info publication="20071029000000">
        <license name="Apache License, Version 2.0" url="http://www.apache.org/licenses/LICENSE-2.0"/>
        <description homepage="http://mina.apache.org/">
            <p>
            Apache MINA is a network application framework which
            helps users develop high performance and high scalability
            network applications easily. It provides an abstract
            event-driven asynchronous API over various transports
            such as TCP/IP and UDP/IP via Java NIO.
            </p>
        </description>
    </info>

    <publications>
        <artifact/>
        <artifact name="source" type="source" ext="zip"/>
    </publications>

    <dependencies>
        <dependency org="org.slf4j" name="slf4j" rev="[1.4.3,1.6.1]" conf="default->default"/>
    </dependencies>

</ivy-module>
