<ivy-module rev="$Id$">

    <info publication="20090322120000">
        <license name="MPL/GPL" url="https://developer.mozilla.org/en/Rhino_License"/>
        <description homepage="http://www.mozilla.org/rhino/">
            Rhino is an open-source implementation of JavaScript
            written entirely in Java. It is typically embedded into Java
            applications to provide scripting to end users.
        </description>
    </info>

    <configurations>
        <conf name="default" description="Configuration for JDK 1.5 and later"/>
        <conf name="jdk14" description="Configuration for JDK 1.4 and earlier"/>
    </configurations>

    <publications>
        <artifact name="js" conf="default"/>
        <artifact name="js-14" conf="jdk14"/>
        <artifact name="source" type="source" ext="zip"/>
        <artifact name="javadoc" type="javadoc" ext="zip"/>
    </publications>

</ivy-module>
