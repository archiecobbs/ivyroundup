<ivy-module rev="$Id$">

    <info publication="20070401120000">
        <license name="Sun Microsystems, Inc. Binary Code License Agreement" url="http://www.sun.com/"/>
        <description homepage="https://sjsxp.dev.java.net/">
        <a href="http://www.jcp.org/en/jsr/detail?id=173">JSR 173</a>
        defines a new Streaming API for XML (StAX). The Sun Java Streaming
        XML Parser (SJSXP) is an efficient implementation of the StAX
        API which is fully compliant with the XML 1.0 and Namespace
        1.0 specifications.
        </description>
    </info>

    <dependencies>
        <dependency org="javax.xml.stream" name="jsr173_api" rev="1.0" conf="default->default"/>
    </dependencies>

</ivy-module>
