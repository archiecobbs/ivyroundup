<ivy-module rev="$Id$">

    <info publication="20100412000000">
        <license name="Apache License, Version 2.0" url="http://www.apache.org/licenses/LICENSE-2.0"/>
        <description homepage="http://code.google.com/p/ivysvn/">

            IvySvn is a resolver for Ivy which uses Subversion for
            storing artifacts for use in Ivy's publish and retrieve
            operations.

        </description>
    </info>

    <publications>
        <artifact name="ivysvnresolver"/>
        <artifact name="source" type="source" ext="zip"/>
    </publications>

    <dependencies>
        <dependency org="org.apache.ant" name="ivy" rev="2.1.0" conf="*->default" />
        <dependency org="com.svnkit" name="svnkit" rev="1.3.2" conf="*->ssh"/>
    </dependencies>
 
</ivy-module>
