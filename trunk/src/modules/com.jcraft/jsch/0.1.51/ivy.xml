<ivy-module rev="$Id$">

    <info publication="20140318000000">
        <license name="BSD Style" url="http://www.jcraft.com/jsch/LICENSE.txt"/>
        <description homepage="http://www.jcraft.com/jsch/">

            <p>
            JSch is a pure Java implementation of <a href="http://ietf.org/html.charters/secsh-charter.html">SSH2</a>.
            </p>

            <p>
            JSch allows you to connect to an sshd server and use port
            forwarding, X11 forwarding, file transfer, etc., and you can
            integrate its functionality into your own Java programs.
            </p>

        </description>
    </info>

    <publications>
        <artifact/>
        <artifact type="source" ext="zip"/>
        <artifact type="javadoc" ext="zip"/>
    </publications>

    <dependencies>
        <dependency org="com.jcraft" name="jzlib" rev="[0,)" conf="default->default"/>
    </dependencies>

</ivy-module>
