<ivy-module rev="$Id$">

    <info publication="20140227000000">
            <license name="The BSD License" url="http://opensource.org/licenses/bsd-license.php"/>
            <description homepage="http://woodstox.codehaus.org/StAX2">
                Stax2 API is an extension to basic Stax 1.0 API that adds significant new functionality, such as full-featured bi-direction validation interface and high-performance Typed Access API.
            </description>
    </info>
    <configurations>
            <conf name="default" visibility="public" description="runtime dependencies and master artifact can be used with this conf"/>
    </configurations>
    <publications>
            <artifact type="jar" ext="jar" />
            <artifact type="source" ext="zip" />
            <artifact type="javadoc" ext="zip" />
    </publications>
    <dependencies>
            <dependency org="javax.xml.stream" name="jsr173_api" rev="1.0" force="true" conf="default->default"/>
    </dependencies>
</ivy-module>
